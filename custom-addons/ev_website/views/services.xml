<odoo>
  <template id="ev_website.services_template" name="services page">

    <t t-name="website.services">
      <t t-call="website.layout">
        <div id="wrap" class="oe_structure oe_empty">
          <section class="s_text_cover o_colored_level o_cc o_cc3" data-snippet="s_text_cover"
            data-name="Text Cover">
            <div class="container-fluid">
              <div class="row o_grid_mode" data-row-count="11">

                <div
                  class="o_grid_item g-height-11 g-col-lg-6 col-lg-6 o_cc o_cc1 o_not_editable d-none d-lg-block o_snippet_mobile_invisible o_colored_level oe_img_bg o_bg_img_center o_bg_img_origin_border_box"
                  style="grid-area: 1 / 7 / 12 / 13; --grid-item-padding-x: 0px; --grid-item-padding-y: 0px; background-image: url(&quot;/web/image/website.s_text_cover_default_image&quot;) !important;"
                  data-invisible="1" />
                <div
                  class="o_grid_item o_cc o_cc1 o_colored_level g-col-lg-7 g-height-8 col-lg-7 border rounded"
                  style="z-index: 1; --grid-item-padding-x: 24px; --grid-item-padding-y: 24px; grid-area: 2 / 2 / 10 / 9; border-width: 1px !important; border-radius: 9px !important; transform: translateY(70px);">
                  <h1>
                    <span style="font-size: 2.625rem;">
                      <span t-out="t('service_head')" />
                    </span>
                  </h1>
                  <p>
                    <span class="h5-fs">
                      <span t-out="t('service_head_sub')" />
                    </span>
                  </p>
                  <p>
                    <br />
                  </p>
                  <a class="btn btn-lg btn-primary">
                    <span t-out="t('contact_us')" />
                  </a>
                </div>
              </div>
            </div>
          </section>
          <section class="s_text_image pt80 pb80 o_colored_level" data-snippet="s_image_text"
            data-name="Text - Image">
            <div class="container">
              <div class="row align-items-center">
                <div class="col-lg-6 pt16 pb16 o_colored_level">
                  <img src="/ev_website/static/src/img/implement_custom.jpg" alt=""
                    class="img img-fluid mx-auto rounded o_we_custom_image"
                    data-mimetype="image/webp" data-original-id="1107"
                    data-original-src="/web/image/1107-7d311333/jose-vazquez-Q5RBHz9cu1A-unsplash.jpg"
                    data-mimetype-before-conversion="image/jpeg" data-resize-width="690"
                    loading="lazy" />
                </div>
                <div class="col-lg-5 offset-lg-1 pt16 o_colored_level pb0">
                  <h2>
                    <span style="font-size: 1.875rem;">
                      <span t-out="t('odoo_implementation_customization')" />
                    </span>
                  </h2>
                  <p>

                  </p>
                  <p>
                    <span t-out="t('odoo_implementation_customization_sub')" />
                  </p>
                  <p>
                    <a class="o_translate_inline" />
                  </p>
                </div>
              </div>
            </div>
          </section>
          <section class="s_text_image o_colored_level pt56 pb0" data-snippet="s_text_image"
            data-name="Text - Image">
            <div class="container">
              <div class="row align-items-center">
                <div class="col-lg-5 pt16 pb16 o_colored_level">
                  <h2 class="h3-fs">
                    <span style="font-size: 1.875rem;">
                      <span t-out="t('reporting_analytics')" />
                    </span>

                  </h2>
                  <p>
                    <span t-out="t('reporting_analytics_sub1')" />  
                  </p>
                  <p>
                    <span t-out="t('reporting_analytics_sub2')" />
                  </p>

                </div>
                <div class="col-lg-6 offset-lg-1 pt16 pb16 o_colored_level">
                  <img src="/ev_website/static/src/img/analytics.jpg"
                    alt="" class="img img-fluid mx-auto rounded o_we_custom_image"
                    data-mimetype="image/webp" data-original-id="1108"
                    data-original-src="/web/image/1108-166db937/jakub-zerdzicki-ykgLX_CwtDw-unsplash.jpg"
                    data-mimetype-before-conversion="image/jpeg" data-resize-width="690"
                    loading="lazy" />
                </div>
              </div>
            </div>
          </section>
          <section class="s_text_image pt80 o_colored_level pb56" data-snippet="s_image_text"
            data-name="Text - Image">
            <div class="container">
              <div class="row align-items-center">
                <div class="col-lg-6 pt16 pb16 o_colored_level">
                  <img src="/ev_website/static/src/img/inventory.jpg" alt=""
                    class="img img-fluid mx-auto rounded o_we_custom_image"
                    data-mimetype="image/webp" data-original-id="1109"
                    data-original-src="/web/image/1109-22db21b7/cova-software-r--jcbaXgek-unsplash.jpg"
                    data-mimetype-before-conversion="image/jpeg" data-resize-width="690"
                    loading="lazy" />
                </div>
                <div class="col-lg-5 offset-lg-1 pt16 pb16 o_colored_level">
                  <h4>
                    <span style="font-size: 1.875rem;">
                      <span t-out="t('inventory_supply_chain')" />
                    </span>
                  </h4>
                  <p>

                  </p>
                  <p>
                    <span t-out="t('inventory_supply_chain1')" />  
                  </p>
                  <p>
                    <span t-out="t('inventory_supply_chain2')" />
                  </p>

                </div>
              </div>
            </div>
          </section>
          <section class="s_text_image pt80 pb80 o_colored_level" data-snippet="s_image_text"
            data-name="Image - Text">
            <div class="container">
              <div class="row o_grid_mode" data-row-count="8">

                <div
                  class="o_colored_level o_grid_item g-col-lg-6 g-height-8 col-lg-6 o_grid_item_image"
                  style="grid-area: 1 / 7 / 9 / 13; z-index: 2;">
                  <img src="/ev_website/static/src/img/our_services.jpg" alt=""
                    class="img img-fluid mx-auto rounded o_we_custom_image"
                    data-mimetype="image/webp" data-original-id="1110"
                    data-original-src="/web/image/1110-05953e70/kelly-sikkema-M98NRBuzbpc-unsplash.jpg"
                    data-mimetype-before-conversion="image/jpeg" data-resize-width="690"
                    loading="lazy" />
                </div>
                <div class="o_colored_level o_grid_item g-col-lg-5 g-height-7 col-lg-5"
                  style="z-index: 1; grid-area: 2 / 1 / 9 / 6;">
                  <h2 class="h3-fs">
                    <span style="font-size: 1.875rem;">
                      <span t-out="t('finance_accounting')" />
                    </span>

                  </h2>
                  <p>
                    <span t-out="t('finance_accounting_sub1')" />
                  </p>
                  <p>
                    <span t-out="t('finance_accounting_sub2')" />
                  </p>

                </div>
              </div>
            </div>
          </section>
          <section class="s_text_image pt80 pb80 o_colored_level" data-snippet="s_text_image"
            data-name="Text - Image">
            <div class="container">
              <div class="row o_grid_mode" data-row-count="8">
                <div class="o_colored_level o_grid_item g-col-lg-5 g-height-7 col-lg-5"
                  style="grid-area: 2 / 8 / 9 / 13; z-index: 2;">
                  <h2 class="h3-fs">
                   <span t-out="t('crm_customer')" />
                  </h2>
                  <p>
                    <span t-out="t('crm_customer_sub1')" />
                  </p>
                  <p>
                    <span t-out="t('crm_customer_sub2')" />
                  </p>
                  <p>


                  </p>

                </div>

                <div
                  class="o_colored_level o_grid_item g-col-lg-6 g-height-8 col-lg-6 o_grid_item_image"
                  style="grid-area: 1 / 1 / 9 / 7; z-index: 1;">
                  <img src="/ev_website/static/src/img/crm.jpg"
                    alt="" class="img img-fluid mx-auto rounded o_we_custom_image"
                    data-mimetype="image/webp" data-original-id="1112"
                    data-original-src="/web/image/1112-1032f849/cytonn-photography-vWchRczcQwM-unsplash.jpg"
                    data-mimetype-before-conversion="image/jpeg" data-resize-width="690"
                    loading="lazy" />
                </div>
              </div>
            </div>
          </section>
          <section class="s_text_image pt80 pb80 o_colored_level" data-snippet="s_image_text"
            data-name="Image - Text">
            <div class="container">
              <div class="row o_grid_mode" data-row-count="8">

                <div
                  class="o_colored_level o_grid_item g-col-lg-6 g-height-8 col-lg-6 o_grid_item_image"
                  style="grid-area: 1 / 7 / 9 / 13; z-index: 2;">
                  <img src="/ev_website/static/src/img/sales.jpg"
                    alt="" class="img img-fluid mx-auto rounded o_we_custom_image"
                    data-mimetype="image/jpeg" data-original-id="1113"
                    data-original-src="/web/image/1113-85ef899c/artem-beliaikin-cE8cwN2A2-c-unsplash.jpg"
                    data-mimetype-before-conversion="image/jpeg" loading="lazy" />
                </div>
                <div class="o_colored_level o_grid_item g-col-lg-5 g-height-7 col-lg-5"
                  style="z-index: 1; grid-area: 2 / 1 / 9 / 6;">
                  <h2 class="h3-fs">
                    <span style="font-size: 1.875rem;">
                      <span t-out="t('sales_commerce')" />
                    </span>

                  </h2>
                  <p>
                    <span t-out="t('sales_commerce_sub1')" />
                  </p>
                  <p>
                    <span t-out="t('sales_commerce_sub2')" />
                  </p>

                </div>
              </div>
            </div>
          </section>


        </div>
      </t>
    </t>
  </template>
</odoo>